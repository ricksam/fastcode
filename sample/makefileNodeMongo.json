{
  "name": "makefileNode",
  "roles": [
    {
      "template": "model",
      "command": "node-express\\src\\core\\models\\[entity].js",
      "overwrite": true
    },
    {
      "template": "repository",
      "command": "node-express\\src\\core\\repositories\\[entity]Repository.js"
    },
    {
      "template": "service",
      "command": "node-express\\src\\core\\services\\[entity]Service.js"
    },
    {
      "template": "controller",
      "command": "node-express\\src\\controllers\\[entity]Controller.js"
    },
    {
      "template": "router",
      "command": "node-express\\src\\routes\\api\\[entity].js",
      "overwrite": false
    },
    {
      "template": "routes.api",
      "command": "node-express\\src\\routes\\api\\index.js",
      "overwrite": true
    },
    {
      "template": "root",
      "command": "node-express\\src\\routes\\front\\root.js",
      "overwrite": true
    },
    {
      "template": "header",
      "command": "node-express\\src\\views\\partials\\header.ejs",
      "overwrite": true
    },
    {
      "template": "view",
      "command": "node-express\\src\\views\\[entity]\\index.ejs"
    }
  ],
